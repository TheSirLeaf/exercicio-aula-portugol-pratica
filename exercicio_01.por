programa {
  funcao inicio() {
    cadeia cidadeSelecionada
    cadeia cidadeMoradia
    cadeia cidadeEstudo
    escreva("Digite a cidade que você mora:\n")
    leia(cidadeSelecionada)
    cidadeMoradia = cidadeSelecionada
    escreva("Digite a cidade que você estuda:\n")
    leia(cidadeSelecionada)
    cidadeEstudo = cidadeSelecionada
    escreva("Você mora em " + cidadeMoradia + " e estuda em " + cidadeEstudo)
  }
}
